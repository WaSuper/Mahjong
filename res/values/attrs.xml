        <enum name="top" value="2"/>
        <enum name="left" value="3"/>
        <enum name="center" value="4"/>
    </attr>   
    <attr name="rotateAngle" format="integer" />
    <attr name="transX" format="dimension" />
    <attr name="transY" format="dimension" />
    <attr name="exText" format="string" />
    <attr name="exTextSize" format="integer"/>
    <attr name="exTextColor" format="color"/>    
    <declare-styleable name="RotateTextView">
        <attr name="rotateAngle" />
