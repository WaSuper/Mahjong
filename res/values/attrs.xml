    </declare-styleable>  
    <declare-styleable name="RotateTextView2">
        <attr name="rotateAngle" />
        <attr name="transX" />
        <attr name="transY" />
        <attr name="position" />
        <attr name="exText" />
    	<attr name="exTextSize" />
    	<attr name="exTextColor" />  
    </declare-styleable>
    <declare-styleable name="ResultListView">
        <attr name="position"/>
    </declare-styleable>
    <declare-styleable name="ResultView">
        <attr name="position"/>
    </declare-styleable>
    <declare-styleable name="PlayerFuncItem">
        <attr name="position"/>
    </declare-styleable>
    <declare-styleable name="WheelPicker">
        <attr name="halfVisibleItemCount"/>
        <attr name="itemTextSize"/>
        <attr name="itemMaximumWidthText"/>
        <attr name="itemTextColor"/>
        <attr name="textGradual"/>
        <attr name="selectedTextColor"/>
        <attr name="selectedTextSize"/>
        <attr name="currentItemPosition"/>
        <attr name="itemWidthSpace"/>
        <attr name="itemHeightSpace"/>
        <attr name="zoomInSelectedItem"/>
